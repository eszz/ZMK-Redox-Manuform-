#include <dt-bindings/zmk/matrix_transform.h>

/ {
    chosen {
        zmk,kscan = &kscan0;
		zmk,matrix_transform = &default_transform;
    };

    default_transform: keymap_transform_0 {
		compatible = "zmk,matrix-transform";
		columns = <5>;
		rows = <14>;
// | SW7  | SW6  | SW5  | SW4  | SW3  | SW2  | SW1  |   | SW1  | SW2  | SW3  | SW4  | SW5  | SW6  | SW7  |
// | SW14 | SW13 | SW12 | SW11 | SW10 | SW9  | SW8  |   | SW8  | SW9  | S10  | SW11 | SW12 | SW13 | SW14 |
// | SW21 | SW20 | SW19 | SW18 | SW17 | SW16 |                 | SW16 | SW17 | SW18 | SW18 | SW20 | SW21 |
// | SW28 | SW27 | SW26 | SW25 | SW24 | SW23 |                 | SW23 | SW24 | SW25 | SW26 | SW27 | SW28 |
// | SW35 | SW34 | SW33 | SW32 |                                             | SW32 | SW33 | SW34 | SW35 |
//                             | SW31 |                               | SW31 |
//                                    | SW30 | SW15 |   | SW15 | SW30 |
//                                    | SW29 | SW22 |   | SW12 | SW22 |
		map = <
RC(0,0) RC(1,0) RC(2,0) RC(3,0) RC(4,0) RC(5,0) RC(6,0) RC(7,0) RC(8,0) RC(9,0) RC(10,0) RC(11,0) RC(12,0) RC(13,0)
RC(0,1) RC(1,1) RC(2,1) RC(3,1) RC(4,1) RC(5,1) RC(6,1) RC(7,1) RC(8,1) RC(9,1) RC(10,1) RC(11,1) RC(12,1) RC(13,1)
RC(0,2) RC(1,2) RC(2,2) RC(3,2) RC(4,2) RC(5,2)                 RC(8,2) RC(9,2) RC(10,2) RC(11,2) RC(12,2) RC(13,2)
RC(0,3) RC(1,3) RC(2,3) RC(3,3) RC(4,3) RC(5,3)                 RC(8,3) RC(9,3) RC(10,3) RC(11,3) RC(12,3) RC(13,3)
RC(0,4) RC(1,4) RC(2,4) RC(3,4)                                                 RC(10,4) RC(11,4) RC(12,4) RC(13,4)
                                RC(4,4)                                 RC(9,4)
                                        RC(5,4) RC(6,2) RC(7,2) RC(8,4)
                                        RC(6,4) RC(6,3) RC(7,3) RC(7,4)
		>;
	};

    kscan0: kscan {
        compatible = "zmk,kscan-gpio-matrix";
        label = "KSCAN";

